  "license": "BSD-3-Clause",
  "main": "index.js",
  "scripts": {
    "docs:clean": "rimraf docs/.vuepress/dist",
    "docs:dev": "cross-env NODE_OPTIONS='-r dotenv-flow/config' vuepress dev docs",
    "docs:build": "cross-env NODE_OPTIONS='-r dotenv-flow/config --experimental-modules --es-module-specifier-resolution=node --max-old-space-size=14000' vuepress build docs",
    "docs:build:limit": "cross-env PACKAGE_LIMIT=5 NODE_OPTIONS='-r dotenv-flow/config --experimental-modules --es-module-specifier-resolution=node --max-old-space-size=14000' vuepress build docs",
    "docs:serve": "ws -d docs/.vuepress/dist --cors.origin '*'",
    "lint": "eslint . --ext .ts --ext .mts --ext .vue",
    "test:dev": "cross-env NODE_ENV=test vitest --config vite.config.test.ts",
    "test": "cross-env NODE_ENV=test vitest run --config vite.config.test.ts"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.11.0",
